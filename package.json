	},
	"bugs": {
		"url": "https://github.com/zws-im/zws/issues"
	},
	"devDependencies": {
		"@semantic-release/exec": "5.0.0",
		"@tsconfig/node14": "1.0.0",
		"@types/node": "14.14.37",
		"cli-ux": "5.5.1",
		"eslint-plugin-prettier": "3.3.1",
		"prettier": "2.2.1",
		"prettier-config-xo": "1.0.3",
		"prisma": "2.21.1",
		"semantic-release": "17.4.2",
		"semantic-release-docker": "2.2.0",
		"ts-json-schema-generator": "0.90.0",
		"ts-node": "9.1.1",
		"type-fest": "1.0.2",
		"typescript": "4.2.4",
		"xo": "0.38.2"
	},
	"license": "Apache-2.0",
	"main": "./tsc_output/src/index.js",
	"name": "@zws.im/zws",
	"engines": {
		"node": "15.x"
	},
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zws-im/zws.git"
	},
	"scripts": {
		"build": "tsc",
		"deploy": "semantic-release",
		"lint": "xo",
		"prebuild": "rm -rf tsc_output",
		"style": "prettier --check .",
		"start": "node .",
		"migrations": "prisma migrate deploy",
		"generate-schemas": "node tsc_output/src/generate-schemas"
	},
	"version": "0.0.0-development",
	"xo": {
		"prettier": true,
		"rules": {
			"prettier/prettier": "off",
			"unicorn/filename-case": "off",
			"import/extensions": "off",
			"unicorn/no-array-reduce": "off",
			"unicorn/no-array-callback-reference": "off",
			"unicorn/no-process-exit": "off"
		}
	},
	"dependencies": {
		"@pizzafox/util": "2.8.0",
		"@prisma/client": "2.21.1",
		"@sentry/node": "6.2.5",
		"consola": "2.15.3",
		"dotenv": "8.2.0",
		"fastify": "3.14.2",
		"fastify-auth": "1.1.0",
		"fastify-bearer-auth": "5.1.0",
		"fastify-error": "0.3.0",
		"fastify-swagger": "4.6.0",
		"ow": "0.23.0"
	}
}
